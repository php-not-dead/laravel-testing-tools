{
    "name": "elph-studio/laravel-testing-tools",
    "type": "library",
    "description": "Laravel Testing Tools library",
    "keywords": ["phpunit", "phpcs", "phpcsfixer"],
    "authors": [{
        "name": "Vitalij Vladimirov",
        "email": "vitalij@cto4sale.com",
        "homepage": "https://www.cto4sale.com"
    }],
    "require": {
        "php": "^8.5",
        "opay-dev/php-linting-tools": "^1.6",
        "phpunit/phpunit": "12.*",
        "mockery/mockery": "1.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/framework": "12.*"
    },
    "autoload": {
        "psr-4": {
            "Elph\\LaravelTestingTools\\": "src"
        }
    },
    "bin": [
        "bin/unit",
        "bin/lint"
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable"
}
