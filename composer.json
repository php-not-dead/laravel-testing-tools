{
    "name": "elph-studio/laravel-testing-tools",
    "type": "library",
    "description": "Laravel Testing Tools library",
    "license": "MIT",
    "keywords": ["phpunit", "testing", "phpcs", "phpcsfixer", "PHP_CodeSniffer", "sniffs", "standards", "lint", "linting", "psr"],
    "authors": [{
        "name": "Elephant Studio",
        "email": "developers@elph.studio",
        "homepage": "https://www.elph.studio"
    }],
    "require": {
        "php": "^8.5",
        "opay-dev/php-linting-tools": "^1.6",
        "phpunit/phpunit": "12.*",
        "mockery/mockery": "1.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/framework": "12.*"
    },
    "autoload": {
        "psr-4": {
            "Elph\\LaravelTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "bin": [
        "bin/unit",
        "bin/lint"
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
